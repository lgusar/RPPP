@model IEnumerable<KoronavirusMvc.Models.Putovanje>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DatumPolaska)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DatumVracanja)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdentifikacijskiBrojNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DatumPolaska)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DatumVracanja)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdentifikacijskiBrojNavigation.IdentifikacijskiBroj)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.SifraPutovanja">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.SifraPutovanja">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.SifraPutovanja">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
