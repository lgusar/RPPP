@model Organizacija


<tr>
    <td>
        <input asp-for="Naziv" class="form-control" data-save="" />
    </td>
    <td>
        <input asp-for="Url" class="form-control" data-save="" />
    </td>
    <td>
        <button type="submit" class="btn btn-sm btn-primary save" title="Spremi"><i class="fas fa-save"></i></button>
    </td>
    <td>
        <button class="btn btn-sm cancel" title="Odustani"><i class="fas fa-undo-alt"></i></button>
    </td>
</tr>
<tr>
    <td colspan="6">
        <div asp-validation-summary="All"></div>
        <input type="hidden" asp-for="SifraOrganizacije" data-save="" />
    </td>
</tr>

